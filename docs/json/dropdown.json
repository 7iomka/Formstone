{"main":["dropdown.js","dropdown.css"],"options":[{"name":"cover","type":"boolean","default":"false","description":"Cover handle with option set"},{"name":"customClass","type":"string","default":"''","description":"Class applied to instance"},{"name":"label","type":"string","default":"''","description":"Label displayed before selection"},{"name":"external","type":"boolean","default":"false","description":"Open options as links in new window"},{"name":"links","type":"boolean","default":"false","description":"Open options as links in same window"},{"name":"mobile","type":"boolean","default":"false","description":"Force desktop interaction on mobile"},{"name":"trim","type":"int","default":"0"}],"events":[],"methods":[{"name":"close","description":"Closes target instance.","example":"$(\".target\").dropdown(\"close\");"},{"name":"defaults","description":"Extends plugin default settings; effects instances created hereafter.","params":[{"name":"options","type":"object","default":"{}","description":"New plugin defaults"}],"example":"$.dropdown(\"defaults\", { ... });"},{"name":"destroy","description":"Removes plugin instance.","example":"$(\".target\").dropdown(\"destroy\");"},{"name":"disable","description":"Disables target instance or option.","params":[{"name":"option","type":"string","default":"null","description":"Target option value"}],"example":"$(\".target\").dropdown(\"disable\", \"1\");"},{"name":"enable","description":"Enables target instance or option.","params":[{"name":"option","type":"string","default":"null","description":"Target option value"}],"example":"$(\".target\").dropdown(\"enable\", \"1\");"},{"name":"open","description":"Opens target instance.","example":"$(\".target\").dropdown(\"open\");"},{"name":"update","description":"Updates instance.","example":"$(\".target\").dropdown(\"update\");"}],"name":"Dropdown","type":"widget","description":"A jQuery plugin for custom select elements.","dependencies":["core.js","touch.js"],"css":[{"name":".fs-dropdo